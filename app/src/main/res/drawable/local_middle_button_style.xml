    <item android:state_enabled="true">
        <bitmap android:src="@drawable/subtab_normal_middle_transparent" />
    </item>
    <item android:state_pressed="true">
        <bitmap android:src="@drawable/subtab_normal_middle_transparent" />
    </item>
    <item android:state_checked="true">
